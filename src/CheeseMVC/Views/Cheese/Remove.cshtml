@{
}

<h1>Remove Cheese</h1>

<p><a asp-controller="Cheese" asp-action="">Cheese List</a></p>

<form method="post">
    @foreach (string name in ViewBag.cheeses)
    {
        <input type="checkbox" name="cheese" value="@name" id="@name"/>
        <label for="@name">@name</label>
        <br />
    }
    <input type="submit" value="Remove Cheese" />
</form>





