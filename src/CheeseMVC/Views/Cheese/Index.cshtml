@{
}

<h1>My Cheese</h1>

<p><a asp-controller="Cheese" asp-action="Add">Add Cheese</a></p>

@if (ViewBag.cheeses.Count == 0)
{
    <p>No cheese yet!</p>

}

<table class="table">

    <tr>
        <th>Name</th>
        <th>Description</th>
    </tr>


    
        @foreach (var cheese in ViewBag.cheeses)
        {
            <tr>
                <th>@cheese.Name</th>
                <th>@cheese.Description</th>
            </tr>
        }
    

</table>